{

  "manifest_version": 2,
  "name": "Speechless",
  "version": "0.01",

  "applications": {
    "gecko": {
      "id": "speechless@matthewsot.outlook.com",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://*.google.com/*document/d/*/edit*"],
      "js": ["jquery.min.js", "moment.js", "utils.js", "docs.js", "speechless.js"]
    }
  ]
}